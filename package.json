{
  "name": "pubnub-mobx",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test:i",
    "build": "rm -rf ./dist/ && rollup -c",
    "test:i": "jest --watch --coverage",
    "test": "jest --coverage",
    "prepublish": "npm run test && npm run build"
  },
  "husky": {
    "pre-commit": "prettier src/* --write",
    "pre-push": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakannimer/pubnub-mobx"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/lodash.memoize": "^4.1.4",
    "@types/pubnub": "^4.0.2",
    "comment-json": "^1.1.3",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "mobx": "^5.5.0",
    "parcel-bundler": "^1.10.1",
    "prettier": "^1.14.3",
    "rollup": "^0.66.4",
    "rollup-plugin-typescript2": "^0.17.1",
    "ts-jest": "^23.10.3",
    "typescript": "^3.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/"
    ]
  },
  "description": "Convert pubnub data streams to a mobx observable boxes.",
  "dependencies": {
    "lodash.memoize": "^4.1.2",
    "pubnub": "^4.21.5"
  }
}
